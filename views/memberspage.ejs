<!-- Begin member Form -->

<div class="page-micro--small band--blue">
    <div class="wrapper">
        <div class="layout">
            <div class="layout__item palm-one-whole lap-and-up-one-whole">
                <h2 class="text-color--white" style="margin-bottom: 0px;font-weight: 500;">Want to be a member?</h2>
                <h4 class="heading-sub text-color--white" style="font-size: 16px;letter-spacing: 1px;font-weight:300;">Enter your email and we'll shoot you a message whenever we have news. No spam, and we won't share it with anyone.</h4>
                <form class="layout page-micro--small" ng-controller="MemberController">
					<div class="layout__item palm-one-whole lap-and-up-one-third">
						<label for="name" class="label--white">Name
							<input type="text" name="name" ng-model="member.name" placeholder="John Smith">
						</label>
					</div><div class="layout__item palm-one-whole lap-and-up-one-third">
						<label for="email" class="label--white">Email
							<input type="email" name="email" ng-model="member.email" placeholder="john_smith@example.com">
						</label>
					</div><div class="layout__item palm-one-whole lap-and-up-one-third">
						<label for="isp" class="label--white">ISP
							<input type="text" name="isp" ng-model="member.ISP" placeholder="Comcast/Time Warner/Verizon/etc">
						</label>
					</div>
					<div class="layout__item palm-one-whole lap-and-up-two-thirds">
						<label for="address" class="label--white">Address
							<input type="text" name="address" ng-model="member.address" placeholder="Street, City, State, Zipcode">
						</label>
					</div><div class="layout__item palm-one-whole lap-and-up-one-third">
						<label for="phone" class="label--white">Phone
							<input type="tel" name="phone" ng-model="member.phoneNumber" placeholder="123-456-7890">
						</label>
					</div>
					<div class="layout__item palm-one-whole lap-and-up-one-whole page-micro--small" style="padding-left:24px;">
						<h4 class="text-normal">Why are you joinging ISPCU?</h4>
						
						<div class="layout--middle">
							<div class="box--light-blue" style="padding:9px;border;solid 1px rgba(0,0,0,0.4);">
							<div class="layout__item palm-one-whole lap-and-up-two-thirds inline-labels">
								
							<label class="label--white" for="checkbox1"><input id="checkbox1" type="checkbox">To support Net Neutrality</label>
							<label class="label--white" for="checkbox2"><input id="checkbox2" type="checkbox">Better Speed</label>
							<label class="label--white" for="checkbox3"><input id="checkbox3" type="checkbox">Lower Price</label>
							<label class="label--white" for="checkbox4"><input id="checkbox4" type="checkbox">More Providers to Choose From</label>
							<label class="label--white" for="checkbox5"><input id="checkbox5" type="checkbox">To ensure I get the service I pay for</label>
							
							</div><div class="layout__item palm-one-whole lap-and-up-one-third">
								<label for="other" class="label--white">Other:
							<input type="text" name="other" ng-model="member.other" placeholder="Misc reason">
						</label>
							
							</div>
						</div>
						</div>
						</div>
                        <div class="layout--center page-micro--small"><div class="layout__item palm-one-whole lap-and-up-one-third"><button class="btn btn--expand btn--secondary" ng-click="memberFunc(member)">Member!</button></div></div>
                         
                    </form>
            </div>
        </div>
    </div>
</div>

<!-- End member Form -->

